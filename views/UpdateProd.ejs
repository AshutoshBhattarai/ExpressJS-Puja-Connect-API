<html lang="en">
<head>
  <title>Insert Products</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
</head>
<body>
<div class="container">
  <h2>Enter Product Details</h2>
  <h2>Update </h2>
  <form action="/product/insert" method="post">
    <div class="form-group">
      <label for="prodName">Name</label>
      <input type="text" class="form-control" id="name" placeholder="Enter name" name="prod_name">
    </div>
    <div class="form-group">
        <label for="prodStock">Stock</label>
        <input type="text" class="form-control" id="name" placeholder="Enter stock" name="prod_stock">
      </div>
      <div class="form-group">
        <label for="prodPrice">Price</label>
        <input type="text" class="form-control" id="name" placeholder="Enter price" name="prod_price">
      </div>
      <div class="form-group">
        <label for="prodImg">Price</label>
        <input type="text" class="form-control" id="name" placeholder="Enter image" name="image">
      </div>
      
    <button type="submit" class="btn btn-default">Submit</button>
  </form>
</div>

</body>
</html>